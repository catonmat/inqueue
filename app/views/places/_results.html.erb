<% places.each do |place| %>
  <%= simple_form_for(place) do |f| %>
    <%= f.text_field :google_place_id, type: "hidden" %>
    <%= f.button :submit, "Start a line", class: "btn btn-link btn-sm" %>
  <% end %>
  {<br>
    name: <%= place['name'] %>,<br>
    address: <%= place['address'] %>,<br>
    latitude: <%= place['latitude'] %>,<br>
    longitude: <%= place['longitude'] %>,<br>
    rating: <%= place['rating'] %>,<br>
    google_place_id: <%= place['google_place_id'] %><br>
  },<br>
<% end %>
